GenerateBaseType(file, array, comment, ByRef check, ByRef slide, ByRef magicCheck){
  GuiControlGet, check
  if check
  {
    FileAppend, %comment%`r`n, %file%, UTF-8
    FileAppend, Show`r`n, %file%
    FileAppend, ` ` BaseType, %file%
    GuiControlGet, Num, , slide
	ArrayLoop(Num, array, file)
	FileAppend,` ` Rarity Normal, %file%
	GuiControlGet, magicCheck
	if magicCheck
	  FileAppend,`,` Magic`r`n, %file%
	else
	  FileAppend,`r`n, %file%
	FileAppend,` ` SetBorderColor 31 199 48`r`n, %file%
  }
}

GenerateEnd(file){
  FileAppend, Hide`r`n, %file%
}

ArrayLoop(bgn, array, file){
  border = % array.Count()
  Loop {
    if A_Index >= %bgn%
	{
	  element = % array[A_Index]
	  if A_Index = %bgn%
	    FileAppend,` `"%element%`", %file%, UTF-8
	  else
	    FileAppend,`,` `"%element%`", %file%, UTF-8
	}
	if A_Index = %border%
	{
	  FileAppend,`r`n, %file%, UTF-8
        break
	}
  }
}

LockUnlockSlider(ByRef check, ByRef slide){
  GuiControlGet, check
  if check
    GuiControl, Enable, slide
  else
    GuiControl, Disable, slide  
}